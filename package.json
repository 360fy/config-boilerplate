{
  "name": "config-boilerplate",
  "version": "1.1.3",
  "description": "Boilerplate for config",
  "repository": "git@github.com:360fy/config-boilerplate.git",
  "scripts": {
    "lint": "eslint ./src/",
    "clean": "rm -rf lib/",
    "compile": "npm run lint && npm run clean && babel -d lib/ src/",
    "prepublish": "npm run compile"
  },
  "author": "Shailendra Sharma",
  "license": "MIT",
  "dependencies": {
    "js-yaml": "^3.6.1",
    "json5": "^0.5.0",
    "lodash": "^4.12.0"
  },
  "eslintConfig": {
    "extends": "360fy"
  },
  "babel": {
    "plugins": [
      "transform-runtime"
    ],
    "presets": [
      "node5"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-plugin-transform-runtime": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-node5": "^11.1.0",
    "babel-preset-react": "^6.5.0",
    "cson": "^3.0.2",
    "eslint": "^2.9.0",
    "eslint-config-360fy": "^1.1.5",
    "eslint-plugin-react": "^5.1.1",
    "hjson": "^1.8.4",
    "properties": "^1.2.1",
    "toml": "^2.3.0",
    "vows": "^0.8.1"
  }
}
