{
  "name": "config-boilerplate",
  "version": "1.1.7",
  "description": "Boilerplate for config",
  "repository": "git@github.com:360fy/config-boilerplate.git",
  "scripts": {
    "lint": "eslint ./src/",
    "clean": "rm -rf lib/",
    "compile": "npm run lint && npm run clean && babel -d lib/ src/",
    "prepublish": "npm run compile"
  },
  "author": "Shailendra Sharma",
  "license": "MIT",
  "dependencies": {
    "js-yaml": "3.7.0",
    "json5": "0.5.0",
    "lodash": "4.17.2"
  },
  "eslintConfig": {
    "extends": "360fy"
  },
  "babel": {
    "plugins": [
      "transform-runtime"
    ],
    "presets": [
      "node5"
    ]
  },
  "devDependencies": {
    "babel-core": "6.18.2",
    "babel-eslint": "7.1.0",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-node5": "11.1.0",
    "babel-preset-react": "6.16.0",
    "cson": "4.0.0",
    "eslint": "3.10.2",
    "eslint-config-360fy": "1.1.12",
    "eslint-plugin-react": "6.7.1",
    "hjson": "2.3.1",
    "properties": "1.2.1",
    "toml": "2.3.0",
    "vows": "0.8.1"
  }
}
